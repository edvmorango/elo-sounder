# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing x sample home page
GET     /                           controllers.HomeController.index
# GET     /                           controllers.PlaneController.get


GET     /plane                          controllers.PlaneController.list
POST    /sounder/deploy                 controllers.SounderController.deploy

GET     /assets/*file       controllers.Assets.versioned(path="/public", file: Asset)

GET     /swagger.json       controllers.ApiHelpController.getResources
GET     /docs/              controllers.Assets.at(path="/public/swagger-ui",file="index.html")
GET     /docs/*file         controllers.Assets.at(path="/public/swagger-ui",file)
